{
  "name": "proofmeet-backend",
  "version": "2.0.8",
  "description": "ProofMeet Version 2.0 - Court Compliance System",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx prisma generate && npx prisma db push --force-reset --accept-data-loss && node dist/index.js",
    "start:old": "npx prisma migrate deploy && npx prisma generate && node server-with-db.js",
    "build": "npx prisma generate && tsc",
    "postinstall": "npx prisma generate",
    "dev": "tsx watch src/index.ts",
    "dev:old": "node server-with-db.js",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "npx prisma studio",
    "migrate:v2": "tsx scripts/migrate-to-v2.ts",
    "seed": "npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@prisma/client": "^5.4.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "qrcode": "^1.5.3",
    "winston": "^3.11.0",
    "typescript": "^5.3.3",
    "prisma": "^5.4.2",
    "@types/node": "^20.10.5",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/compression": "^1.7.5",
    "@types/qrcode": "^1.5.5",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": "18.x"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  }
}
